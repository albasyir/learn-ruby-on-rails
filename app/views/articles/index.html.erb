<h1>Articles</h1>
<%= link_to "Create New Article", new_article_path %>

<ul>
<% @articles.each do |article| %>
  <li>
    <a href="/articles/<%= article.id %>">
      <%= article.title %> (<%= article_path(article) %>)
    </a>
  </li>
    <li>
      <%= link_to article.title, article %> (use link_to)
    </li>
<% end %>
</ul>